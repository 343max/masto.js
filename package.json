{
  "name": "@lagunehq/core",
  "private": false,
  "version": "1.3.0",
  "description": "Mastodon API client for TypeScript",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "tslint -c tslint.yml 'src/**/*.{ts,tsx}'",
    "build": "tsc",
    "build:docs": "typedoc ./src --out ./docs --excludeNotExported && touch ./docs/.nojekyll",
    "publish": "npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lagunehq/core.git"
  },
  "keywords": [
    "mastodon"
  ],
  "author": "neetshin",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/lagunehq/core/issues"
  },
  "homepage": "https://github.com/lagunehq/core#readme",
  "dependencies": {
    "eventemitter3": "^3.1.0",
    "node-fetch": "^2.2.0",
    "query-string": "^6.1.0",
    "websocket": "^1.0.26"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.1.2",
    "@types/query-string": "^6.1.0",
    "@types/websocket": "^0.0.39",
    "typedoc": "^0.12.0",
    "typescript": "^3.0.1"
  }
}
